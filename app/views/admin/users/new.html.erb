<h1>New User</h1>
<% form_for @user, :url => admin_users_path, :html => {:multipart => true} do |f| %>
<%= render :partial => 'admin/users/form.html.erb', :locals => {:f => f} %>
<% end %>

<% content_for :scripts do %>
<%= static_asset_javascript 'javascripts/views/admin/users/new', 'cache/views/admin/users/new' %>
<% end %>

<% content_for :styles do %>
<%= static_asset_stylesheet 'stylesheets/views/admin/users/new', 'cache/views/admin/users/new' %>
<% end %>

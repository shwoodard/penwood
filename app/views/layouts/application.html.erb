<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE html PUBLIC "-//W3C//DTD XHTML 1.0 Strict//EN"
    "http://www.w3.org/TR/xhtml1/DTD/xhtml1-strict.dtd">
<html xmlns="http://www.w3.org/1999/xhtml" xml:lang="en" lang="en">
<head>
<title>Penwood Partners - Transforming Relationships</title>
<meta http-equiv="Content-Type" content="text/html; charset=UTF-8" />
<meta http-equiv="X-UA-Compatible" content="IE=7" />
<%= static_asset_stylesheet 'stylesheets/common/global', 'cache/common/global' %>
<%= yield :styles %>
<%= yield :google_js %>
</head>
<body>
  <div id="singleColWpr">
    <div id="singleCol">
      <div id="singleColLiner">
        <div id="gblTop">
          <div id="gblTopLiner">
            <%= render :partial => 'partials/gbl_session_nav.html.erb' unless guest? %>
            <% unless logged_in? || member? %><div class="gblSessionNav"><%= link_to 'Sign in', contact_path-%></div><% end %>
            <a href="<%= root_path-%>"><span id="gblLogo" class="block float"></span></a>
            <%= render :partial => 'partials/gbl_nav.html.erb' %>
          </div>
        </div>
        <div id="gblContent">
          <% if flash[:notice] %><div class="flashNotice"><%= flash[:notice] %></div><% end %>
          <%= yield %>
        </div>
        <div id="gblFooter">
          <div id="gblFooterLiner">
            <%= render :partial => 'partials/gbl_footer.html.erb' %>
          </div>
        </div>
      </div>
    </div>
  </div>
<%= static_asset_javascript 'javascripts/common/global', 'cache/common/global' %>
<%= yield :scripts %>
</body>
</html>
